+</head>
+<body>
+<script>
+    for (let i = 0; i <= 7; i++) {
+        for (let j = 0; j < 20; j++) {
+            if (i === 0 || i === 7 || j === 0 || j ===19) {
+                document.write("*");
+            } else {
+                document.write("&ensp;");
